{
  "version": 2,
  "builds": [
    {
      "src": "admin-interface/package.json",
      "use": "@vercel/next",
      "config": {
        "installCommand": "npm install --legacy-peer-deps",
        "buildCommand": "npm run build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/admin-interface/$1"
    }
  ]
}
