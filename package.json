{
  "name": "requirements-gathering-agent",
  "version": "1.0.0",
  "description": "Comprehensive PMBOK project management documentation generation agent powered by LLM",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "pmbok",
    "project-management",
    "documentation",
    "requirements",
    "llm",
    "azure",
    "github"
  ],
  "dependencies": {
    "@azure-rest/ai-inference": "^1.0.0-beta.6",
    "@azure/core-auth": "^1.9.0",
    "axios": "^1.9.0",
    "dotenv": "^16.5.0",
    "openai": "^5.0.1"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/node": "^22.15.29",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },  "bin": {
    "requirements-gathering-agent": "dist/cli.js",
    "requirements-agent": "dist/cli.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
